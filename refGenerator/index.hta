<html>

<head>
    <title>Reference Generator</title>
    <HTA:APPLICATION WINDOWSTATE="normal" SYSMENU="yes" SHOWINTASKBAR="yes" SELECTION="no" NAVIGABLE="yes"
        MINIMIZEBUTTON="yes" MAXIMIZEBUTTON="no" INNERBORDER="yes" CONTEXTMENU="no" CAPTION="yes" SCROLL="no"
        ICON="icon.ico" BORDERSTYLE="normal" APPLICATIONNAME="Reference Generator" BORDER="thik" SCROLL="no"
        SINGLEINSTANCE="yes" ID="oHTA">
</head>

<body background="background.jpg">
    <div class="p">
        <span style="font-size: 20px; font-weight: bold; color: #c10;" id="connection">Not connected yet!</span>
    </div><br />
    <div class="inputform">
        <label for="name">Client Name:</label>
        <input type="text" id="name" placeholder="Anonymous" />
        <label for="email">Email:</label>
        <input type="email" id="email" placeholder="" />
    </div><br />
        <button onclick="myfunction()" id="generate">Generate</button>
        <button onclick="clipboard()" id="copy">copy</button>
    <button class="cmd" onclick="button()">
    <img alt="settings" height="20px" width="20px" src="data:image/png;base64," />
    </button>
    <div id="buttonCon" class="none">
        <button id="connect" onclick="checkConnection()">Reconnect</button>
        <button id="query" onclick="runQ()">Query</button>
        <button id="reset" onclick="reset()">Reset</button>
        <button class="cache" onclick="clearChache()">Clear Cache</button>
        <button class="deleteData" onclick="deleteData()">Delete Data</button>
        <button class="refresh" onclick="refresh()">Refresh</button>
    </div>
    <div id="result"></div>
    <div id="log"></div>

</body>
<style>
    body {
        font-family: sans-serif;
        background-size: cover;
        background-position: center;
        background-repeat: no-repeat;
        background-attachment: fixed;
    }
    #name {
        width: 200px;
        height: 30px;
        font-size: 18px;
        font-weight: bold;
    }
    #button {
        width: 10px;
        height: 10px;
        position: absolute;
        right: 10px;
        top: 10px;
    }   
    #result {
        width: 100%;
        height: auto;
        color: #087eb4;
        font-size: 50px;
        text-align: center;
        font-weight: bold;
    }
    #buttonCon {
        position: absolute;
        bottom: 10px;
        right: 10px;
        width: 100%;
        background-color: #fff;
        padding: 10px;
        box-shadow: 0px 0px 10px #000;
    }
    .none {
        display: none;
    }
    .block {
        display: block;
    }
    .cmd{
        font-size: 18px;
        font-weight: bold;
        margin: 10px;
        right: 5px;
        position: absolute;
        top: 15px;
    }
    .refresh{
        background: #1e0;
        color: #fff;
    }
    .deleteData{
        background: #e55;
        color: #fff;
    }
    .cache{
        background: #a1a0ae;
        color: #fff;
    }
    button {
        font-size: 18px;
        font-weight: bold;
        margin: 10px;
        right: 5px;
        bottom: 5px;
    }
    #settings {
        width: 100%;
        text-align: left;
    }
    #reset {
        background: #e10;
        color: #fff;
    }
    #copy,
    #generate {
        width: 100px;
    }
    .p {
        width: 100%;
        height: auto;
        top: 0;
    }
    #connection {
        display: inline-block;
    }
    #connect {
        display: inline-block;
        position: absolute;
        height: 30px;
        font-size: 18px;
        font-weight: bold;
        right: 0;
    }
    #email {
        width: 300px;
        height: 30px;
        font-size: 18px;
        font-weight: bold;
    }
    .td {
        display: inline-block;
        height: 20px;
        text-align: left;
        padding: 5px;
    }
    .th {
        display: inline-block;
        height: 20px;
        text-align: left;
        padding-bottom: 5px;
        padding-top: 5px;
        font-weight: bold;
    }
    .td.no0 {
        width: 50px;
    }
    .td.no1 {
        width: 200px;
    }
    .td.no2 {
        width: 350px;
    }
    .td.no3 {
        width: 150px;
    }
    .td.no4 {
        width: 150px;
    }
    .td.no5 {
        width: 150px;
    }
    .tr {
        display: inline-block;
        width: 100%;
        align-items: center;
        justify-content: center;
        height: 20px;
    }
</style>
<script language="javascript" src="share.js"></script>
<script language="VBScript" src="index.vb"></script>
</html>